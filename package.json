{
  "name": "aestudy-website",
  "version": "1.0.0",
  "description": "AE Website",
  "author": "Automattic Theme Team",
  "license": "GPL-2.0-or-later",
  "keywords": [
    "WordPress",
    "Theme"
  ],
  "homepage": "https://github.com/Automattic/_s#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Automattic/_s.git"
  },
  "bugs": {
    "url": "https://github.com/Automattic/_s/issues"
  },
  "dependencies": {
    "bootstrap": "^5.0.2"
  },
  "devDependencies": {
    "@wordpress/scripts": "^16.1.4",
    "browser-sync": "^2.27.3",
    "dir-archiver": "^1.1.1",
    "rtlcss": "^3.2.1",
    "sass": "^1.35.1"
  },
  "rtlcssConfig": {
    "options": {
      "autoRename": false,
      "autoRenameStrict": false,
      "blacklist": {},
      "clean": true,
      "greedy": false,
      "processUrls": false,
      "stringMap": []
    },
    "plugins": [],
    "map": false
  },
  "scripts": {
    "clean": "rm -rf node_modules package-lock.json",
    "clean:install": "npm run clean && npm i",
    "build": "sass sass/:./ --source-map --style compressed",
    "watch": "sass sass/:./ --source-map --style expanded -w",
    "start": "npm run watch",
    "compile:css": "sass sass/:./ --style compressed && stylelint *.css --fix || true && stylelint *.css --fix",
    "compile:rtl": "rtlcss style.css style-rtl.css",
    "fix:scss": "wp-scripts lint-style sass/**/*.scss --fix",
    "lint:scss": "wp-scripts lint-style sass/**/*.scss",
    "lint:php": "phpcs --standard=WordPress /Users/renan/development/_local-wp/ae-study/app/public/wp-content/themes/AE/",
    "format": "phpcbf --standard=WordPress /Users/renan/development/_local-wp/ae-study/app/public/wp-content/themes/AE/",
    "bundle": "dir-archiver --src . --dest ../_s.zip --exclude .DS_Store .stylelintrc.json .eslintrc .git .gitattributes .github .gitignore README.md composer.json composer.lock node_modules vendor package-lock.json package.json .travis.yml phpcs.xml.dist sass style.css.map",
    "packages-update": "wp-scripts packages-update"
  }
}
